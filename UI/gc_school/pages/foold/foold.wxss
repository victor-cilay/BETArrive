page {
    background-color: #fff;
}

.notData {
    align-items: center;
    display: flex;
    flex-direction: column;
    height: 100%;
    justify-content: center;
}

.notData .icon {
    filter: grayscale(100);
    width: 80%;
}

.head {
    color: #fff;
    display: flex;
    height: 0rpx;
}

.head .head-img {
    width: 200rpx;
}

.head .head-img image {
    border-radius: 20rpx;
    height: 160rpx;
    margin: 18rpx;
    width: 160rpx;
}

.head .head-detail {
    flex-grow: 1;
    font-size: 26rpx;
}

.head .head-detail .info {
    height: 60rpx;
    line-height: 60rpx;
}

.head .head-detail .info text {
    border-right: 1rpx solid #eee;
    padding: 0 10rpx;
}

.head .head-detail .info text:last-child {
    border: none;
}

.head .head-detail .publicMsg {
    height: 60rpx;
    line-height: 60rpx;
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
    width: 500rpx;
}

.head .actives,.head .item {
    height: 60rpx;
}

.head .item {
    display: flex;
    flex-wrap: wrap;
    line-height: 60rpx;
    width: 500rpx;
}

.head .active-icon {
    color: #fff;
    height: 40rpx;
    line-height: 40rpx;
    margin-right: 10rpx;
    margin-top: 10rpx;
    text-align: center;
    width: 40rpx;
}

.head .active-text {
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
    width: 450rpx;
}

.manjian {
    background-color: #ff8dc1;
}

.xindian {
    background-color: #75fb6d;
}

.zhekou {
    background-color: #ff111a;
}

.daijinquan {
    background-color: #fbd305;
}

.xinyonghu {
    background-color: #f93fe9;
}

.peisong {
    background-color: #67e1ef;
}

.lingdaijin {
    background-color: #56fb43;
}

.zengsong {
    background-color: #fd86ed;
}

.tab {
    border-bottom: 2rpx solid #eee;
    display: flex;
    height: 8rpx;
    position: relative;
}

.tab .tab-item {
    line-height: 78rpx;
    text-align: center;
    width: 160rpx;
}

.tab .border {
    background-color: #ffd161;
    height: 4rpx;
    left: 30rpx;
    position: absolute;
    top: 76rpx;
    transition: all .5s;
    width: 100rpx;
}

.food {
    border-top: 1px solid #eee;
    display: flex;
    height: 100%;
}

.food-class {
    background-color: #f5f9fc;
    width: 180rpx;
}

.food-class .active {
    background-color: #fff;
}

.food-class .food-class-item {
    align-items: center;
    box-sizing: border-box;
    display: flex;
    font-size: 28rpx;
    height: 100rpx;
    justify-content: center;
    line-height: 40rpx;
    padding: 10rpx 20rpx;
    position: relative;
    width: 180rpx;
}

.food-class .food-class-item:last-child {
    margin-bottom: 120rpx;
}

.food-class .food-class-item .text {
    max-height: 80rpx;
    overflow: hidden;
    text-align: center;
}

.food-class .food-class-item .dot {
    background-color: red;
    border-radius: 50%;
    color: #fff;
    font-size: 20rpx;
    height: 30rpx;
    line-height: 30rpx;
    position: absolute;
    right: 4rpx;
    text-align: center;
    top: 4rpx;
    width: 30rpx;
}

.food-detail {
    flex-grow: 1;
    font-size: 26rpx;
    padding: 0 14rpx;
}

.food-detail .title {
    font-weight: 700;
    line-height: 50rpx;
    margin-bottom: 20rpx;
    margin-top: 30rpx;
    padding: 0 20rpx;
    text-overflow: ellipsis;
}

.food-detail .title-group {
    padding-bottom: 30rpx;
}

.food-detail .title-group:last-child {
    margin-bottom: 120rpx;
}

::-webkit-scrollbar {
    color: transparent;
    height: 0;
    width: 0;
}

.food-info {
    display: flex;
    margin-bottom: 30rpx;
}

.food-info:last-child {
    margin-bottom: 0;
}

.food-info .img {
    text-align: center;
    width: 120rpx;
}

.food-info .img image {
    border-radius: 15rpx;
    height: 100rpx;
    width: 100rpx;
}

.food-info .info {
    flex-grow: 1;
    margin-left: 10rpx;
}

.info .name {
    font-size: 28rpx;
}

.info .note {
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
}

.price-count {
    display: flex;
    height: 50rpx;
    line-height: 50rpx;
}

.price-count .price {
    color: red;
    flex-grow: 1;
    font-size: 30rpx;
}

.price-count .count {
    display: flex;
    flex-direction: row-reverse;
    flex-grow: 1;
    line-height: 60rpx;
}

.price-count .count .num {
    font-size: 30rpx;
    line-height: 50rpx;
}

.cart {
    background-color: #000;
    bottom: 0;
    height: 110rpx;
    line-height: 110rpx;
    position: fixed;
    width: 100%;
}

.cart .cart-container {
    color: #eee;
    display: flex;
    position: relative;
}

.cart .del-price-money {
    display: flex;
    flex-direction: column;
    flex-grow: 1;
    font-size: 28rpx;
    padding-left: 140rpx;
}

.cart .del-price-money .money {
    font-size: 36rpx;
    height: 60rpx;
    line-height: 110rpx;
}

.cart .del-price-money .del-price {
    font-size: 24rpx;
    height: 30rpx;
    line-height: 30rpx;
}

.cart .start-del {
    background-image: linear-gradient(-135deg,#fff5ca,#fff2cc);
    color: grey;
    font-size: 30rpx;
    font-weight: 700;
    text-align: center;
    width: 200rpx;
}

.cart .cart-img {
    background-color: #666;
    border-radius: 50%;
    height: 100rpx;
    left: 20rpx;
    position: absolute;
    text-align: center;
    top: -24rpx;
    width: 100rpx;
    z-index: 9;
}

.cart .cart-img image {
    height: 60rpx;
    margin-bottom: 20rpx;
    vertical-align: middle;
    width: 60rpx;
}

.cart .has-food {
    background-image: linear-gradient(134deg,#ffde87,#ffc400);
    color: #333;
}

.cart .cart-list {
    background-color: #fff;
    bottom: 110rpx;
    position: absolute;
    width: 100%;
}

.cart .cart-list-header {
    background-color: #eee;
    color: #333;
    font-size: 28rpx;
    height: 60rpx;
    line-height: 60rpx;
    padding: 0 30rpx;
    text-align: right;
}

.cart-list-item {
    max-height: 400rpx;
    overflow: scroll;
}

.cart-list-item .item {
    align-items: center;
    display: flex;
    margin: 20rpx 0;
}

.cart-list-item .item .cart-detail {
    flex: 1;
    line-height: 40rpx;
    text-align: center;
}

.cart-count {
    align-items: center;
}

.cart-count,.modal {
    display: flex;
}

.modal {
    background-color: rgba(0,0,0,.8);
    bottom: 0;
    flex-direction: column;
    left: 0;
    position: fixed;
    right: 0;
    top: 0;
    z-index: 999;
}

.modal .top {
    flex-grow: 1;
}

.modal .bottom {
    background-color: #fff;
    font-size: 28rpx;
    padding: 30rpx;
}

.modal .modal-active {
    display: flex;
    margin-bottom: 10rpx;
}

.modal .modal-icon {
    color: #fff;
    height: 40rpx;
    line-height: 40rpx;
    text-align: center;
    width: 40rpx;
}

.modal .modal-text {
    margin-left: 10rpx;
}

.modal .info {
    margin-top: 40rpx;
}

.modal .info .title {
    font-size: 40rpx;
    font-weight: 700;
}

.modal .info text {
    border-right: 1rpx solid #eee;
    padding: 0 10rpx;
}

.modal .info text:first-child {
    padding-left: 0;
}

.modal .info text:last-child {
    border: none;
}

