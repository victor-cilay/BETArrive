}

.newest_toast {
    color: #000;
    height: 125rpx;
    left: 3%;
    position: absolute;
    top: 20%;
    width: 400rpx;
}

.newest_toast image {
    border-radius: 50%;
    flex-shrink: 0;
    height: 35rpx;
    margin-right: 10rpx;
    margin-top: 10rpx;
    width: 35rpx;
}

.newest_toast .item {
    background: hsla(0,0%,75%,.8);
    border-radius: 30rpx;
    color: #fff;
    display: flex;
    font-size: 22rpx;
    line-height: 50rpx;
    margin-top: 10rpx;
    max-width: 320rpx!important;
    padding: 0 10rpx;
}

.newest_toast .item .word {
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
}

.text-orange {
