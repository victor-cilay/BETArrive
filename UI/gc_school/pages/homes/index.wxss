.search_box {
    background: #fff;
    border-radius: 10rpx;
    box-shadow: 0 2px 10px 0 rgba(0,0,0,.1);
    display: flex;
    font-size: 42rpx;
    height: 70rpx;
    justify-content: space-between;
    line-height: 70rpx;
    margin: -40rpx 3% 0;
    padding: 0 20rpx;
    position: absolute;
    width: 94%;
    z-index: 999;
}

.search_input {
    color: #000!important;
    font-size: 30rpx;
    margin-top: 16rpx;
    width: 85%;
}

.location {
    align-items: center;
    background-color: #64edac;
    color: #fff;
    display: flex;
    font-size: 26rpx;
    padding: 15rpx 10rpx;
}

.location image {
    margin-right: 10rpx;
    width: 50rpx;
}

.page-guide {
    box-sizing: border-box;
    margin-top: 30rpx;
}

.content {
    background: #fff0d8;
    color: #000;
    font-size: 30rpx;
    padding: 70rpx 0 30rpx;
    text-align: center;
}

#tops {
    padding-bottom: 150rpx;
}

.page-guide .nav {
    background-color: #fafafa;
    font-size: 16px;
    height: 44px;
    width: 100%;
}

.page-guide .nav,.page-guide .nav .nav-item {
    align-items: center;
    display: flex;
}

.page-guide .nav .nav-item {
    height: 100%;
    justify-content: center;
    position: relative;
    text-align: center;
    width: 50%;
}

.page-guide .nav .nav-item.active {
    color: #ff2d4b;
}

.page-guide .nav .nav-item.active:after {
    background: #ff2d4b;
    border-radius: 999px;
    content: " ";
    display: block;
    height: 2px;
    left: 50%;
    position: absolute;
    top: 39px;
    transform: translate(-50%);
    width: 33px;
}

.page-guide .shop-wrapper {
    flex: 1;
    width: 100%;
}

.page-guide .shop-wrapper .shop {
    display: flex;
    flex-direction: column;
    transition: transform .2s linear;
    transition: transform .2s linear,-webkit-transform .2s linear;
    width: 100%;
}

.page-guide .shop-wrapper .shop-banner {
    background-repeat: no-repeat;
    background-size: 100% 100%;
    border-radius: 20rpx;
    bottom: 100rpx;
    height: 143px;
    width: 100%;
}

.swiper-slide {
    background-position: 50%;
    background-size: cover;
}

.page-guide .shop-wrapper .shop-banner .rider-nearby-content {
    align-items: center;
    background-color: #623c15;
    border-radius: 55px;
    bottom: 90px;
    box-sizing: border-box;
    color: #fff;
    display: flex;
    font-size: 13px;
    height: 25px;
    justify-content: center;
    left: 50%;
    line-height: 18px;
    padding: 0 11px;
    position: absolute;
    right: 0;
    transform: translate3d(-50%,0,0);
    white-space: nowrap;
    width: 142px;
    z-index: 100;
}

.page-guide .shop-wrapper .shop-banner .rider-nearby-icon {
    background-image: url(https://xs01.meituan.net/banma_paotui/dist/images/paotui-rider.png);
    background-size: 20px 17px;
    display: inline-block;
    height: 17px;
    margin-right: 6px;
    width: 20px;
}

.page-guide .shop-wrapper .shop-banner .rider-nearby-content .num {
    color: #f6ce00;
}

.page-guide .shop-wrapper .shop-banner .swiper-container {
    height: 100%;
    margin-left: auto;
    margin-right: auto;
    overflow: hidden;
    padding: 0;
    position: relative;
    width: 100%;
    z-index: 1;
}

.page-guide .shop-wrapper .swiper-container .swiper-wrapper {
    box-sizing: initial;
    display: flex;
    height: 100%;
    position: relative;
    transition-property: transform;
    transition-property: transform,-webkit-transform;
    width: 100%;
    z-index: 1;
}

.page-guide .shop-wrapper .swiper-container .swiper-wrapper .swiper-slide {
    border-radius: 20rpx;
    height: 100%;
    width: 100%;
}

.page-guide .shop-wrapper .good-edit .good-input {
    flex: 1;
    padding: 0 6px;
    position: relative;
}

.page-guide .shop-wrapper .good-edit .good-input-placeholder.active {
    display: inline-flex;
}

.page-guide .shop-wrapper .good-edit .good-input-placeholder {
    align-items: flex-start;
    color: #999;
    display: none;
    font-size: 18px;
    height: 110px;
    line-height: 1;
    width: 100%;
}

.page-guide .shop-wrapper .good-edit .good-input-icon {
    height: 18px;
    margin-right: 7px;
    width: 16px;
}

.page-guide .shop-wrapper .good-edit .good-input-content {
    align-items: center;
    display: none;
    height: 110px;
    justify-content: center;
    padding-bottom: 7px;
    width: 100%;
}

.page-guide .shop-wrapper .good-edit .good-input-content.active {
    display: flex;
}

.page-guide .shop-wrapper .good-edit .good-input-content .good-input-textarea {
    border: none;
    color: #333;
    flex: 1;
    font-size: 18px;
    height: 100%;
    line-height: 26px;
    overflow-x: hidden;
    overflow-y: auto;
    padding-left: 6px;
    resize: none;
}

.page-guide .shop-wrapper .good-edit .good-input-content .good-input-submit {
    background-image: url(https://xs01.meituan.net/banma_paotui/dist/images/good-submit.png);
    background-repeat: no-repeat;
    background-size: 100% 100%;
    font-size: 15px;
    height: 72px;
    margin-top: 18px;
    width: 73px;
}

.paotui-order {
    border: 1px solid #87ceeb;
    border-radius: 50px;
    bottom: 80px;
    height: 50px;
    position: fixed;
    right: 10px;
    text-align: center;
    width: 50px;
    z-index: 9999;
}

.paotui-order .icon {
    color: #87ceeb;
    font-size: 24px;
}

.paotui-order .order-text {
    color: #87ceeb;
    font-size: 12px;
    margin-top: -8px;
}

.page-guide .tagbg {
    height: 60rpx;
    left: -20rpx;
    position: absolute;
    top: -20rpx;
    width: 60rpx;
}

.good-edit {
    border-radius: 10rpx;
    box-sizing: border-box;
    margin: 20rpx auto 30rpx;
    overflow: hidden;
    width: 100%;
}

.good-edit .bgmain {
    background-color: #f2f2f2;
    color: #878787;
}

.good-edit .text-sm {
    font-size: 24rpx;
}

.rank .t-right {
