}

.cu-item {
    font-size: 30rpx;
}

.address {
    font-size: 28rpx;
}

.box {
    border-radius: 10rpx;
    margin: 30rpx;
}

.status_btn {
    background-color: #fef5f5;
    border-radius: 20rpx;
    color: #ff4c4d;
    font-size: 24rpx;
    padding: 10rpx 0;
    text-align: center;
    width: 100rpx;
}

.time {
    color: #666;
    font-size: 26rpx;
    margin-top: 10rpx;
}

.addr_box {
    background-color: #f9f9f9;
    border-radius: 20rpx;
    margin-top: 20rpx;
    padding: 30rpx;
}

.addr {
    font-size: 30rpx;
    font-weight: 700;
}

.circle {
    background-color: #fff;
    border: 6rpx solid #3792f5;
    border-radius: 50%;
    flex-shrink: 0;
    height: 22rpx;
    width: 22rpx;
}

.bule {
    border: 6rpx solid #3792f5;
}

.orange {
    border: 6rpx solid #fb4119;
}

.cuIcon-moreandroid {
    color: grey;
    margin-left: -5rpx;
}

.operate_box .operate:first-child {
    margin-left: auto;
}

.operate {
    border: 1rpx solid #e5e5e5;
    border-radius: 28rpx;
    color: #323232;
    padding: 10rpx 30rpx;
}

.active {
    background-color: #ff4c4d;
    border: none;
    color: #fff;
}