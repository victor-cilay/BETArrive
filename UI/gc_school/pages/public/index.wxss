.images-box {
    border-radius: 10rpx;
    width: 100%;
}

.imageCount {
    height: 80rpx;
    line-height: 80rpx;
}

.images {
    align-items: center;
    display: flex;
    height: 240rpx;
}

.addImageIcon {
    background-color: #f7f7f7;
    line-height: 200rpx;
    text-align: center;
}

.addImageIconHover {
    background-color: #c0c4cc;
}

.image {
    border-radius: 10rpx;
    color: #ff6344;
    font-size: 80rpx;
    height: 200rpx;
    margin-right: 20rpx;
    width: 30%;
}

.del {
    height: 60rpx;
    width: 60rpx;
}

page {
    background: #f9f9f9;
}

.adds {
    color: #ff6444!important;
    font-size: 90rpx!important;
}

.icons {
    height: 1.3em!important;
    width: 1.3em!important;
}

.border-radius {
    border-top-left-radius: 20rpx;
    border-top-right-radius: 20rpx;
}

.border-bottom-radius {
    border-bottom-left-radius: 20rpx;
    border-bottom-right-radius: 20rpx;
}

.text-sm {
    font-size: 30rpx!important;
}

.money_box {
    background-color: #232122;
    border-bottom-left-radius: 50rpx;
    border-top-left-radius: 50rpx;
    color: #fff;
    height: 90rpx;
    line-height: 90rpx;
    width: 65%;
}

.money_box,.pay {
    padding-left: 30rpx;
}

.pay {
    font-size: 30rpx;
}

.pay .money {
    font-size: 40rpx;
}

.btn_bg {
    background-color: #ff4c4d!important;
    border-bottom-right-radius: 50rpx!important;
    border-top-right-radius: 50rpx!important;
    border-radius: 0;
    color: #fff;
    font-size: 30rpx;
    height: 90rpx;
    line-height: 90rpx;
    width: 35%;
}

textarea {
    background: #f9f9f9;
    height: 5.6em!important;
    padding: 20rpx 0!important;
    text-indent: 1em;
}

.white {
    background: #fff!important;
}

.up_text {
    font-size: 30rpx;
    font-weight: 700;
    height: 20rpx!important;
    padding-bottom: 50rpx!important;
    width: 100%!important;
}

.slide-line-bottom {
    display: flex;
    justify-content: space-between;
}

.cu-list.menu>.cu-item .content>text[class*="cuIcon"] {
    width: auto;
}

.right-bt {
    align-content: flex-end;
    justify-content: flex-end;
    width: 50%;
}

.solids {
    background: #f9f9f9;
    border-radius: 6rpx;
    margin-bottom: 20rpx;
    margin-right: 20rpx;
    overflow: hidden;
    position: relative;
}

.solids>text {
    align-items: center;
    bottom: 0;
    color: var(--grey);
    display: flex;
    flex-direction: column;
    font-size: 52rpx;
    justify-content: center;
    left: 0;
    margin: auto;
    position: absolute;
    right: 0;
    top: 0;
}

.detals {
    padding: 15rpx 0;
}

.cu-items {
    background: #fff;
    padding: 0 30rpx;
}

#opli {
    width: 40%;
}

#addre {
    align-items: center;
    display: flex;
    width: 60%;
}

#addre>view {
    text-align: right;
    text-overflow: ellipsis;
    width: 100%;
}

#addre>view,.point {
    overflow: hidden;
    white-space: nowrap;
}

.remark {
    min-height: 70rpx;
}

.del_remark {
    font-size: 24rpx;
    margin-bottom: 30rpx;
}

.choose {
    background: #feefec;
    color: #fd6141;
    font-weight: 700;
    line-height: 100rpx;
    margin-left: 5%;
    text-align: center;
    width: 90%;
}

.num_add,.num_cut {
    border: 1rpx solid #f2f2f2;
    border-radius: 10rpx;
    font-size: 34rpx;
    height: 60rpx;
    line-height: 60rpx;
    text-align: center;
    width: 60rpx;
}

.num_add {
    background: #ff6444;
    color: #fff;
}

.input {
    font-size: 30rpx;
    height: 80rpx;
    padding: 0 20rpx;
}

.store_name {
    background: #fff;
    font-size: 32rpx;
    font-weight: 800;
}

.tips {
    align-items: center;
    color: #ff6344;
    display: flex;
    flex-direction: column;
    font-size: 30rpx;
    justify-content: center;
}

.goods_list {
    background: #f9f9f9;
    border-radius: 20rpx;
}

.goods_list .main_img {
    border-radius: 10rpx;
    flex-shrink: 0;
    height: 100rpx;
    margin-right: 20rpx;
    width: 100rpx;
}

.goods_list .goods_name {
    -webkit-line-clamp: 2;
    -webkit-box-orient: vertical;
    color: #333;
    display: -webkit-box;
    font-family: PingFang SC;
    font-size: 28rpx;
    font-weight: 500;
    max-width: 320rpx;
    overflow: hidden;
    text-overflow: ellipsis;
}

.goods_list .info {
    display: flex;
    flex-direction: column;
    height: 100rpx;
}

.goods_list .price {
    color: #ed5a25;
    flex-shrink: 0;
    margin-left: auto;
}

.goods_list .nums {
    color: #333;
    font-family: PingFang SC;
    font-size: 24rpx;
    font-weight: 400;
    margin-top: auto;
}

.w100 {
    width: 100%;
}

.h100 {
    height: 100%;
}

.cu-item.box_fee {
    min-height: 60rpx!important;
}

.cu-item.box_fee::after {
    border-bottom-width: 0;
    display: none;
}

.jiqu {
    background: #fff;
}

.jiqu .add {
    background: #ff4c4d;
    border-radius: 34px;
    color: #fff;
    font-family: PingFang SC;
    font-size: 28rpx;
    font-weight: 400;
    height: 68rpx;
    justify-content: center;
    margin-left: auto;
    width: 199rpx;
}

.jiqu .spec {
    border: 1rpx solid #eee;
    border-radius: 20rpx;
    margin: 20rpx;
    overflow: unset;
    padding: 20rpx;
    position: relative;
}

.jiqu .spec .del {
    background: #ff4c4d;
    border-radius: 100rpx;
    color: #fff;
    font-weight: 900;
    height: 40rpx;
    justify-content: center;
    left: -20rpx;
    position: absolute;
    top: -20rpx;
    width: 40rpx;
}

.jiqu .item {
    padding: 0!important;
}

.jiqu .cu-list.menu>.item:after,.jiqu .item {
    border: 0 solid #fff;
}

.jiqu .nums {
    border: 1rpx solid #f7f5f6;
    border-radius: 4rpx;
    justify-content: space-between;
    width: 300rpx;
}

.jiqu .nums .jia,.jiqu .nums .jian {
    background: #f7f5f6;
    flex-shrink: 0;
    font-size: 50rpx;
    height: 70rpx;
    justify-content: center;
    width: 70rpx;
}

.jiqu .nums input {
    text-align: center;
}

.jiqu .code {
    background: #f9f9f9;
    border-radius: 10rpx;
    font-family: PingFang SC;
    font-size: 30rpx;
    font-weight: 400;
    height: 74rpx;
    padding-left: 20rpx;
    width: 450rpx;
}

.filter {
    filter: grayscale(1);
}

checkbox.blue.checked .uni-checkbox-input,checkbox.blue[checked] .wx-checkbox-input,radio.blue.checked .uni-radio-input,radio.blue[checked] .wx-radio-input,switch.blue.checked .uni-switch-input,switch.blue[checked] .wx-switch-input {
    background-color: #ff6344!important;
    border-color: #ff6344!important;
    color: #fff!important;
}

.storeType {
    background-color: #fff;
    border-radius: 30rpx 30rpx 0 0;
    height: 80rpx;
    justify-content: space-around;
}

.storeType .item {
    align-items: center;
    display: flex;
    font-size: 32rpx;
    font-weight: 900;
    height: 100%;
    justify-content: center;
    width: 50%;
}

.storeType .item.cur {
    background: #ff6344;
    color: #fff;
}