page {
    background: #fff!important;
}

.bgmains {
    color: #fff;
}

.title {
    line-height: 120rpx;
    margin-top: -10rpx;
}

.list,.title {
    background: #fff;
    margin-bottom: 30rpx;
}

.list {
    display: flex;
    font-size: 30rpx;
    line-height: 60rpx;
    margin-top: -15rpx;
    padding: 40rpx 0 20rpx;
    text-align: center;
}

.list_item {
    border-right: 1rpx solid #f2f2f2;
    width: 50%;
}

.list_item .num {
    font-size: 36rpx;
    font-weight: 700;
}

.menu {
    background: #fff;
    display: flex;
    flex-wrap: wrap;
}

.menu_item {
    border-bottom: 1rpx solid #f2f2f2;
    border-right: 1rpx solid #f2f2f2;
    font-size: 30rpx;
    padding: 50rpx 0;
    text-align: center;
    width: 50%;
}

.menu_item image {
    height: 95rpx;
    margin-bottom: 20rpx;
    width: 95rpx;
}

.notice_fixed {
    bottom: 0;
    position: fixed;
    width: 100%;
}

.notice_fixed .bg {
    border-radius: 20rpx;
    width: 100%;
}

.notice_fixed .notice_con {
    left: 30rpx;
    position: absolute;
    top: 30rpx;
    width: 92%;
}

.notice_fixed .icon {
    height: 50rpx;
    margin-right: 10rpx;
    width: 50rpx;
}

.notice_fixed .cuIcon-close {
    font-size: 40rpx;
    font-weight: 700;
}

.notice_fixed .titles {
    flex-grow: 1;
}

.notice_fixed .close_btn {
    background-color: #fff!important;
    border: 2rpx solid #ff4c4d;
    color: #ff4c4d!important;
    margin-right: 20rpx;
}

.notice_fixed .recharge_btn {
    background-color: #ff4c4d;
    border-radius: 50rpx;
    color: #fff;
    font-size: 24rpx;
    height: 60rpx;
    line-height: 60rpx;
    text-align: center;
    width: 160rpx;
}

.main {
    align-items: center;
    background-repeat: no-repeat;
    background-size: 100% 100%;
    display: flex;
    flex-direction: column;
    height: 860rpx;
    padding-top: 180rpx;
    position: absolute;
    top: 0;
    width: 100%;
}

.main .avatar {
    background: #fff;
    border: 8rpx solid #fff;
    border-radius: 10rpx;
    height: 140rpx;
    width: 140rpx;
}

.main .name {
    color: #fff;
    font-family: PingFangSC-Semibold,PingFang SC;
    font-size: 32rpx;
    font-weight: 600;
}

.main .zi {
    color: #fff;
    font-family: PingFangSC-Regular,PingFang SC;
    font-size: 22rpx;
    font-weight: 400;
}

.main .price_info {
    background-image: url(https://test.fkynet.net/wximage/store/store_bg1.png);
    background-repeat: no-repeat;
    background-size: 100% 100%;
    border-radius: 30rpx;
    box-shadow: 1px 1px 10px grey;
    height: 340rpx;
    margin-top: 30rpx;
    width: 690rpx;
}

.main .price_info .key {
    color: grey;
    font-family: PingFangSC-Regular,PingFang SC;
    font-size: 28rpx;
    font-weight: 400;
}

.main .price_info .val {
    color: #333;
    font-family: WeChat-Sans-Std-Medium,WeChat-Sans-Std;
    font-size: 48rpx;
    font-weight: 500;
}

.main .price_info .tx_btn {
    align-items: center;
    background: #ff4c4d;
    border-radius: 16rpx;
    color: #fff;
    display: flex;
    height: 64rpx;
    justify-content: center;
    margin-left: auto;
    width: 166rpx;
}

.main .price_info .tj {
    justify-content: space-around;
    width: 100%;
}

.main .price_info .tj .key {
    font-size: 24rpx;
}

.main .price_info .tj .val {
    font-size: 36rpx;
    margin-top: 10rpx;
}

.tit {
    color: #333;
    font-family: PingFangSC-Semibold,PingFang SC;
    font-size: 38rpx;
    font-weight: 600;
    margin-bottom: 20rpx;
}

.nav_list {
    margin-top: 30rpx;
}

.nav_list .nav {
    border-bottom: 2rpx solid #f1f1f1;
    color: #333;
    font-family: PingFangSC-Regular,PingFang SC;
    font-size: 34rpx;
    font-weight: 400;
}

.nav_list .nav .icon {
    height: 35rpx;
    margin-right: 30rpx;
    width: 35rpx;
}

.nav_list .nav .cuIcon-right {
    margin-left: auto;
}

.bgmains {
    background: transparent!important;
    position: absolute;
}

.tixian .bgmains {
    color: #fff;
}

.tixian .header {
    align-items: center;
    background-image: url(https://test.fkynet.net/wximage/store/tixian_bg.png);
    background-repeat: no-repeat;
    background-size: 100% 100%;
    display: flex;
    flex-direction: column;
    height: 760rpx;
    padding-top: 230rpx;
    width: 750rpx;
}

.tixian .header .key {
    color: #fff;
    font-family: PingFangSC-Regular,PingFang SC;
    font-size: 28rpx;
    font-weight: 400;
}

.tixian .header .val {
    color: #fff;
    font-family: WeChat-Sans-Std-Medium,WeChat-Sans-Std;
    font-size: 60rpx;
    font-weight: 500;
}

.tixian .box {
    background-color: #fff;
    border-radius: 40rpx 40rpx 0 0;
    height: 300rpx;
    margin-top: -130rpx;
    position: relative;
    top: 0;
    z-index: 9999999;
}

.tixian .box .money {
    background: #f7f8f9;
    border-radius: 16rpx;
    height: 80rpx;
    margin-top: 40rpx;
    width: 690rpx;
}

.tixian .box .money .key {
    color: #333;
    font-family: PingFangSC-Medium,PingFang SC;
    font-size: 32rpx;
    font-weight: 500;
}

.tixian .box .tips {
    color: #333;
    font-family: PingFangSC-Regular,PingFang SC;
    font-size: 24rpx;
    font-weight: 400;
}

.tixian .submit {
    align-items: center;
    background: linear-gradient(270deg,#ff716c,#ff484a);
    border-radius: 40rpx;
    color: #fff;
    display: flex;
    font-family: PingFangSC-Medium,PingFang SC;
    font-size: 32rpx;
    font-weight: 500;
    height: 88rpx;
    justify-content: center;
    margin-left: 15rpx;
    margin-top: 40rpx;
    width: 660rpx;
}